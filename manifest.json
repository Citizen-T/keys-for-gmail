{
    "manifest_version": 2,
    "name": "Keys for Gmail",
    "version": "0.1",
    "browser_action": {
        "defualt_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://mail.google.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}